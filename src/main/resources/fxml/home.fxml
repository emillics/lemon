<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.effect.DropShadow?>
<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="768.0"
           minWidth="1024.0"
           prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/16"
           xmlns:fx="http://javafx.com/fxml/1">
    <TabPane fx:id="tabModule" side="LEFT" StackPane.alignment="TOP_LEFT" styleClass="tab-module">
        <effect>
            <DropShadow color="#00000099" height="10.0" radius="4.5" width="10.0"/>
        </effect>
        <tabs>
            <Tab closable="false">
                <graphic>
                    <ImageView fitWidth="30" fitHeight="30" preserveRatio="true">
                        <Image url="/images/spider.png"/>
                    </ImageView>
                </graphic>
                <TabPane fx:id="tabSpider">
                    <tabs>
                        <Tab closable="false">
                            <graphic>
                                <ImageView preserveRatio="true" fitWidth="25">
                                    <Image url="/images/youtube.png"/>
                                </ImageView>
                            </graphic>
                        </Tab>
                        <Tab closable="false">
                            <graphic>
                                <ImageView preserveRatio="true" fitWidth="25">
                                    <Image url="/images/douyin.png"/>
                                </ImageView>
                            </graphic>
                        </Tab>
                        <Tab closable="false">
                            <graphic>
                                <ImageView preserveRatio="true" fitWidth="25">
                                    <Image url="/images/bilibili.png"/>
                                </ImageView>
                            </graphic>
                        </Tab>
                    </tabs>
                </TabPane>
            </Tab>

            <Tab closable="false">
                <graphic>
                    <ImageView fitWidth="30" fitHeight="30" preserveRatio="true">
                        <Image url="/images/publish.png"/>
                    </ImageView>
                </graphic>
            </Tab>
        </tabs>
    </TabPane>
    <VBox prefWidth="69" maxWidth="69" minWidth="69" maxHeight="-Infinity" StackPane.alignment="BOTTOM_LEFT"
          alignment="BOTTOM_CENTER">
        <Button maxWidth="Infinity" prefHeight="60" style="-fx-background-color: transparent;-fx-border: null">
            <graphic>
                <ImageView fitWidth="30" fitHeight="30" preserveRatio="true">
                    <Image url="/images/settings.png"/>
                </ImageView>
            </graphic>
        </Button>
    </VBox>
</StackPane>